{
  "manifest_version": 3,
  "name": "Flow AI Autocomplete Extension",
  "version": "1.0",
  "description": "AI-powered autocomplete using Gemini API.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
